@Article{Balogh2012,
  author    = {J{\'o}zsef Balogh and B{\'e}la Bollob{\'a}s and Robert Morris},
  title     = {Graph bootstrap percolation},
  journal   = {Random Structures and Algorithms},
  year      = {2012},
  volume    = {41},
  number    = {4},
  pages     = {413--440},
  month     = {12},
  issn      = {1042-9832},
  abstract  = {Graph bootstrap percolation is a deterministic cellular automaton which was introduced by Bollob{\'a}s in 1968, and is defined as follows. Given a graph H, and a set G ? E(K n) of initially 'infected' edges, we infect, at each time step, a new edge e if there is a copy of H in K n such that e is the only not-yet infected edge of H. We say that G percolates in the H-bootstrap process if eventually every edge of K n is infected. The extremal questions for this model, when H is the complete graph K r, were solved (independently) by Alon, Kalai and Frankl almost thirty years ago. In this paper we study the random questions, and determine the critical probability p c(n,K r) for the K r-process up to a poly-logarithmic factor. In the case r = 4 we prove a stronger result, and determine the threshold for p c(n,K 4).},
  day       = {1},
  doi       = {10.1002/rsa.20458},
  keywords  = {Bootstrap percolation, Graph theory, Weak saturation},
  language  = {English (US)},
  publisher = {John Wiley and Sons Ltd},
}

@Article{Simon2019,
  author         = {\~Simon, Marek and Huraj, Ladislav and Dirgov{\~a} Lupt{\~a}kov{\~a}, Iveta and Posp{\'i}chal, Ji\:r{\'i}},
  title          = {Heuristics for Spreading Alarm throughout a Network},
  journal        = {Applied Sciences},
  year           = {2019},
  volume         = {9},
  number         = {16},
  issn           = {2076-3417},
  abstract       = {This paper provides heuristic methods for obtaining a burning number, which is a graph parameter measuring the speed of the spread of alarm, information, or contagion. For discrete time steps, the heuristics determine which nodes (centers, hubs, vertices, users) should be alarmed (in other words, burned) and in which order, when afterwards each alarmed node alarms its neighbors in the network at the next time step. The goal is to minimize the number of discrete time steps (i.e., time) it takes for the alarm to reach the entire network, so that all the nodes in the networks are alarmed. The burning number is the minimum number of time steps (i.e., number of centers in a time sequence alarmed &ldquo;from outside&rdquo;) the process must take. Since the problem is NP complete, its solution for larger networks or graphs has to use heuristics. The heuristics proposed here were tested on a wide range of networks. The complexity of the heuristics ranges in correspondence to the quality of their solution, but all the proposed methods provided a significantly better solution than the competing heuristic.},
  article-number = {3269},
  doi            = {10.3390/app9163269},
  url            = {https://www.mdpi.com/2076-3417/9/16/3269},
}

