@Article{Bessy2018,
  author   = {St\'ephane Bessy and Anthony Bonato and Jeannette Janssen and Dieter Rautenbach and Elham Roshanbin},
  title    = {Bounds on the burning number},
  journal  = {Discrete Applied Mathematics},
  year     = {2018},
  volume   = {235},
  pages    = {16 - 22},
  issn     = {0166-218X},
  abstract = {Motivated by a graph theoretic process intended to measure the speed of the spread of contagion in a graph, Bonato et al. (Burning a Graph as a Model of Social Contagion, Lecture Notes in Computer Science 8882 (2014) 13-22) define the burning number b(G) of a graph G as the smallest integer k for which there are vertices x1,?,xk such that for every vertex u of G, there is some i?{1,?,k} with distG(u,xi)=k-i, and distG(xi,xj)=j-i for every i,j?{1,?,k}. For a connected graph G of order n, they prove that b(G)=2n-1, and conjecture b(G)=n. We show that b(G)=3219?n1-?+2719? and b(G)=12n7+3?1.309n+3 for every connected graph G of order n and every 0<?<1. For a tree T of order n with n2 vertices of degree 2, and n=3 vertices of degree at least 3, we show b(T)=(n+n2)+14+12 and b(T)=n+n=3. Furthermore, we characterize the binary trees of depth r that have burning number r+1.},
  doi      = {https://doi.org/10.1016/j.dam.2017.09.012},
  keywords = {Graphs, Burning, Distance domination},
  url      = {http://www.sciencedirect.com/science/article/pii/S0166218X17304353},
}

@InProceedings{Kare2019,
  author    = {Kare, Anjeneya Swami and Vinod Reddy, I.},
  title     = {Parameterized {Algorithms} for {Graph} {Burning} {Problem}},
  booktitle = {Combinatorial {Algorithms}},
  year      = {2019},
  editor    = {Colbourn, Charles J. and Grossi, Roberto and Pisanti, Nadia},
  series    = {Lecture {Notes} in {Computer} {Science}},
  pages     = {304--314},
  address   = {Cham},
  publisher = {Springer International Publishing},
  abstract  = {The Graph Burning problem is defined as follows. At time {\textbackslash}(t=0{\textbackslash}), no vertex of the graph is burned. At each time {\textbackslash}(t {\textbackslash}ge 1{\textbackslash}), we choose a vertex to burn. If a vertex is burned at time t, then at time {\textbackslash}(t+1{\textbackslash}) each of its unburned neighbors becomes burned. Once a vertex is burned then it remains in that state for all subsequent steps. The process stops when all vertices are burned. The burning number of a graph is the minimum number of steps needed to burn all the vertices of the graph.Computing the burning number of a graph is {\textbackslash}({\textbackslash}mathsf \{NP\}{\textbackslash})-complete even on bipartite graphs or trees of maximum degree three. In this paper we study this problem from the parameterized complexity perspective. We show that the problem is fixed-parameter tractable ({\textbackslash}({\textbackslash}mathsf \{FPT\}{\textbackslash})) when parameterized by the distance to cluster or neighborhood diversity. We further study the complexity of the problem on restricted classes of graphs. We show that Graph Burning can be solved in polynomial time on cographs and split graphs.},
  doi       = {10.1007/978-3-030-25005-8_25},
  isbn      = {9783030250058},
  language  = {en},
}

